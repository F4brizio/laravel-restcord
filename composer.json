{
  "name": "more-cores/laravel-restcord",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LaravelRestcord\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": ">=5.0",
    "illuminate/contracts": ">=5.0",
    "restcord/restcord": "0.*",
    "laravel/socialite": "3.*"
  },
  "require-dev": {
    "symfony/var-dumper": "dev-master",
    "mockery/mockery": "1.0.0-alpha1",
    "phpunit/phpunit": "5.*",
    "socialiteproviders/discord": "2.*",
    "codeclimate/php-test-reporter": "dev-master",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "minimum-stability": "stable",
  "suggest": {
    "socialiteproviders/discord": "Bootstraps Restcord's API library with the currently OAuth'd user"
  }
}
